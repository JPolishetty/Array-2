class Solution {
    public List<Integer> findDisappearedNumbers(int[] nums) {
        List<Integer> result = new ArrayList<>();
        if(nums == null || nums.length == 0){
            return result;
        }
        //for range (1,n), indexes are (0, n-1), check for numbers by making (number-1) index negative
        for(int i = 0;i< nums.length; i++){
            //if the index+1 number already visited, it will be negative
            int num = Math.abs(nums[i]) - 1;
            if(nums[num] > 0){
                nums[num] = nums[num] * -1;
            }  
        }
        //After visiting all nums, if we have positive number, then (index+1) is not present
        for(int i=0;i<nums.length; i++){
            if(nums[i] > 0){
                result.add(i+1);
            }
        }
        return result;
    }
}
